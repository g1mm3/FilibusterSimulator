<Window x:Class="FilibusterSimulator.GameWindows.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilibusterSimulator.GameWindows"
        mc:Ignorable="d"
        Title="LoadWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnLoad1" Height="50" Margin="250, 10" Grid.Row="0" Click="BtnLoad_Click" />
        <Button x:Name="btnLoad2" Height="50" Margin="250, 10" Grid.Row="1" Click="BtnLoad_Click" />
        <Button x:Name="btnLoad3" Height="50" Margin="250, 10" Grid.Row="2" Click="BtnLoad_Click" />
        <Button x:Name="btnLoad4" Height="50" Margin="250, 10" Grid.Row="3" Click="BtnLoad_Click" />
        <Button x:Name="btnReturn" Height="50" Content="Назад" Margin="300, 10" Grid.Row="4" Click="BtnReturn_Click" />
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
    </Grid>
</Window>
